# Gemini API
GEMINI_API_KEY=your_api_key_here
NEXT_PUBLIC_API_KEY=your_api_key_here

# WalletConnect (Get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# IPFS/Pinata (Get from https://pinata.cloud/)
NEXT_PUBLIC_PINATA_API_KEY=your_pinata_api_key
NEXT_PUBLIC_PINATA_API_SECRET=your_pinata_secret

# Infura IPFS (Alternative - Get from https://infura.io/)
NEXT_PUBLIC_IPFS_PROJECT_ID=your_infura_project_id
NEXT_PUBLIC_IPFS_PROJECT_SECRET=your_infura_secret

# Ethereum RPC (Optional - defaults to public RPC)
NEXT_PUBLIC_RPC_URL=https://eth.llamarpc.com

# Etherscan API (Get from https://etherscan.io/apis)
NEXT_PUBLIC_ETHERSCAN_API_KEY=your_etherscan_api_key

# GitHub Token (Optional - for private repos)
NEXT_PUBLIC_GITHUB_TOKEN=your_github_token
